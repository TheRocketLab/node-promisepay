{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "globals": {
    "ROOT": true,
    "TEST_ROOT": true,
    "SRC": true
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "strict": 0,
    "import/no-unresolved": 0,
    "no-dupe-class-members": 2,
    "padded-blocks": 0,
    "consistent-return": 0,
    "max-len": [2, 120, 2, {
      "ignoreUrls": true,
      "ignoreComments": true
    }]
  }
}
